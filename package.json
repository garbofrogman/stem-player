{
  "name": "Stem Player",
  "description": "Easily load stem tracks to play along to.",
  "version": "0.1.1",
  "main": "build/waveform-playlist.umd.js",
  "author": "garbofrogman, Naomi Aro",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.27.2",
    "@babel/core": "^7.27.4",
    "@babel/plugin-transform-runtime": "^7.27.4",
    "@babel/preset-env": "^7.27.2",
    "ajv": "^8.17.1",
    "babel-loader": "^10.0.0",
    "gh-pages": "^6.3.0",
    "mkdirp": "^3.0.1",
    "parallel-webpack": "^2.6.0",
    "prettier": "3.5.3",
    "sass": "^1.89.2",
    "webpack": "^5.99.9",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2"
  },
  "directories": {
    "build": "build",
    "lib": "lib",
    "css": "styles"
  },
  "dependencies": {
    "@babel/runtime": "^7.27.6",
    "cors": "^2.8.5",
    "event-emitter": "^0.3.5",
    "fade-curves": "^1.0.2",
    "fade-maker": "^1.0.3",
    "inline-worker": "^1.1.0",
    "lodash.assign": "^4.2.0",
    "lodash.defaultsdeep": "^4.6.1",
    "lodash.forown": "^4.4.0",
    "tone": "^15.1.22",
    "uuid": "^11.1.0",
    "virtual-dom": "^2.1.1",
    "webaudio-peaks": "^1.0.0"
  },
  "scripts": {
    "clean": "rm -Rf dist && rm -Rf lib && rm -Rf build && rm -Rf styles",
    "build": "npm run clean && webpack",
    "preversion": "npm run clean",
    "version": "npm run build && git add -A dist",
    "postversion": "git push && git push --tags",
    "prepare": "npm run compile ",
    "compile": "babel src --out-dir lib",
    "webpack:server": "webpack serve --static dist --color --host 0.0.0.0 --config webpack.config.js",
    "start": "npm run webpack:server & cd dist/js && node fileserver.js",
    "dev": "npm run webpack:server",
    "website": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/garbofrogman/stem-player"
  },
  "keywords": [
    "waveform",
    "audio",
    "audacity",
    "stem",
    "tracks",
    "multitrack",
    "playlist",
    "music",
    "editor",
    "record",
    "recording",
    "player",
    "webaudio",
    "music"
  ],
  "homepage": "https://github.com/garbofrogman/stem-player"
}
