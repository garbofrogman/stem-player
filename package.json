{
  "name": "Stem Player",
  "description": "Easily load stem tracks to play along to.",
  "version": "0.1.1",
  "main": "build/waveform-playlist.umd.js",
  "author": "garbofrogman, Naomi Aro",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "",
    "@babel/core": "",
    "@babel/plugin-transform-runtime": "",
    "@babel/preset-env": "",
    "ajv": "",
    "babel-loader": "",
    "mkdirp": "",
    "prettier": "",
    "sass": "",
    "webpack": "",
    "webpack-cli": "",
    "webpack-dev-server": ""
  },
  "directories": {
    "build": "build",
    "lib": "lib",
    "css": "styles"
  },
  "dependencies": {
    "@babel/runtime": "",
    "cors": "",
    "event-emitter": "",
    "fade-curves": "",
    "fade-maker": "",
    "ini": "",
    "inline-worker": "",
    "lodash.assign": "",
    "lodash.defaultsdeep": "",
    "lodash.forown": "",
    "lrclib-api": "",
    "paroles": "",
    "tone": "",
    "uuid": "",
    "virtual-dom": "",
    "webaudio-peaks": ""
  },
  "scripts": {
    "clean": "rm -Rf dist && rm -Rf lib && rm -Rf build && rm -Rf styles",
    "build": "npm run clean && webpack",
    "preversion": "npm run clean",
    "version": "npm run build && git add -A dist",
    "postversion": "git push && git push --tags",
    "prepare": "npm run compile ",
    "compile": "babel src --out-dir lib",
    "webpack:server": "webpack serve --static dist --color --host 0.0.0.0 --config webpack.config.js",
    "start": "npm run webpack:server & cd dist/js && node fileserver.js",
    "dev": "npm run webpack:server",
    "website": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/garbofrogman/stem-player"
  },
  "keywords": [
    "waveform",
    "audio",
    "audacity",
    "stem",
    "tracks",
    "multitrack",
    "playlist",
    "music",
    "editor",
    "record",
    "recording",
    "player",
    "webaudio",
    "music"
  ],
  "homepage": "https://github.com/garbofrogman/stem-player"
}
